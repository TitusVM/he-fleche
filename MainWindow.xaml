<Window x:Class="HE_Fleche.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HE_Fleche"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Emploi du temps" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Button_Click" Height="30" Width="110"/>
        <Button Content="Voir les notes" HorizontalAlignment="Center" VerticalAlignment="Top" Click="Button_Click" Height="30" Width="110" Margin="0,237,0,0"/>
        <Button Content="Quitter" HorizontalAlignment="Center" VerticalAlignment="Top" Click="Button_Click" Height="30" Width="110" Margin="0,272,0,0"/>
        <Image Margin="286,37,0,0" Stretch="Fill" Source="pack://siteoforigin:,,,/logo.png" Height="200" VerticalAlignment="Top" HorizontalAlignment="Left" Width="200"/>
        <TextBox HorizontalAlignment="Left" Margin="10,10,0,0" Text="HE-Flèche" TextWrapping="Wrap" VerticalAlignment="Top" Width="184" TextChanged="TextBox_TextChanged" Height="46" FontFamily="Segoe UI Black" FontSize="36" TextDecorations="{x:Null}" Background="White"/>
    </Grid>
</Window>
